{
  "key": "",
  "manifest_version": 2,

  "name": "WebPass",
  "description": "Web extension for zx2c4's pass",
  "version": "0.1",
  "applications": {
    "gecko": {
      "id": "webpass@itimmer.nl",
      "strict_min_version": "50.0"
    }
  },

  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Alt+Shift+L"
      }
    }
  },
  "permissions": [
    "activeTab",
    "nativeMessaging"
  ]
}
